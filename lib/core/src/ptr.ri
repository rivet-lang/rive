extend none : Stringable {
    [inline]
    public func to_string(&self) string {
        return "none";
    }
}

func cstr_runes_count(s: [*]uint8, size: usize) usize {
	mut i: usize := 0;
	mut l: usize := 0;
	while i < size {
		l += 1;
		i += unsafe { s[i] }.len_utf8();
	}
	return l;
}
