extern (C) {
    func __acrt_iob_func(index: u32) *mut FILE;
}

pub let stdin: *mut FILE = unsafe { __acrt_iob_func(0) };
pub let stdout: *mut FILE = unsafe { __acrt_iob_func(1) };
pub let stderr: *mut FILE = unsafe { __acrt_iob_func(2) };
