extern (C) {
    fn __acrt_iob_fn(index: u32) *mut FILE;
}

pub var stdin: *mut FILE = unsafe { __acrt_iob_fn(0) };
pub var stdout: *mut FILE = unsafe { __acrt_iob_fn(1) };
pub var stderr: *mut FILE = unsafe { __acrt_iob_fn(2) };
