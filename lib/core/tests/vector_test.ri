test "dynamic arrays: push and pop" {
    mut vec := ["A", "B"];
    @assert(vec.len == 2);

    vec.push("C");
    @assert(vec.len == 3);

    p := vec.pop();
    @assert(p == "C");

    @assert(vec.len == 2);
}

test "dynamic arrays: clear" {
    mut vec := ["A", "B"];
    @assert(vec.len == 2);
    @assert(vec[0] == "A");
    @assert(vec[1] == "B");

    vec.clear();
    @assert(vec.len == 0);
    @assert(vec == []);

    vec.push("C");
    @assert(vec.len == 1);
    @assert(vec[0] == "C");
}
