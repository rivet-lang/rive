pub class Error : ToString {
    pub msg: string;

    pub fn to_string(self) string {
        let type_name = "Error";
        if self.msg == "" {
            return type_name;
        }
        return "{}: {}".fmt(type_name, self.msg);
    }
}

pub class OutOfMemoryError : Error { }

pub class RuneFromU32Error : Error { }
