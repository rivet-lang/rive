struct Xyz {
    a: [5][4]i32;
    b: [8]fn();
    c: [10][7]fn();
}

fn new_array() [4]i32 {
    return [1, 2, 3, 4]!;
}

test "array literal" {
    let lit = [1, 2, 3, 4]!;
    @assert(lit == [1, 2, 3, 4]!);
    @assert(lit.len == 4);
}

test "array returned from function" {
    let arr = new_array();
    @assert(arr == [1, 2, 3, 4]!);
    @assert(arr.len == 4);
}
