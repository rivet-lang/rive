struct Xyz {
    a: [5][4]int32;
    b: [8]proc ();
    c: [10][7]proc ();
}

proc new_array() -> [4]int32 {
    return [1, 2, 3, 4];
}

test "array constructor" {
    lit := [5]int(init: 10);
    @assert(lit == [10, 10, 10, 10, 10]);
}

test "array literal" {
    lit := [1, 2, 3, 4];
    @assert(lit == [1, 2, 3, 4]);
    @assert(lit.len == 4);
}

test "array returned from procedure" {
    arr := new_array();
    @assert(arr == [1, 2, 3, 4]);
    @assert(arr.len == 4);
}
