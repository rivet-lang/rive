test "dynamic array from fixed array" {
    arr := [1, 2, 3];
    dyn_array := arr[:].to_dynamic_array();
    dyn_array.push(2);
    @assert(dyn_array.len == 4);

    dyn_array2 := dyn_array.clone();
    @assert(dyn_array2.len == 4);
    @assert(dyn_array == dyn_array2);
}

test "dynamic array constructor" {
    {
        da := []int32();
        da.push(5);
        @assert(da[0] == 5);
    }
    {
        da := []int32(cap: 2);
        da.push(10);
        @assert(da.cap == 2);
        @assert(da[0] == 10);
    }
}
