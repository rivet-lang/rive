struct D {
    m: uint8 = 155;
}

struct E < D {
    m1: int32;
}

test "embedded structs: using embedded fields" {
    e := E();
    @assert(e.m == 155);
    @assert(e.m1 == 0);

    e2 := E(1, 2);
    @assert(e2.m == 1);
    @assert(e2.m1 == 2);

    e3 := E(m1: 100);
    @assert(e3.m == 155);
    @assert(e3.m1 == 100);
}
