class OuterClass {
    class InnerClass {
        abc: int32;
    }

    inner: InnerClass;

    func method(self) { }

    func associated_fn() { }

    func caller(self) {
        self.method();
        OuterClass.associated_fn();
    }
}

test "inner type" {
    let o = OuterClass(inner: OuterClass.InnerClass(5));
    @assert(o.inner.abc == 5);
}
