struct OuterStruct {
    struct InnerStruct {
        abc: int32;

        proc method(self) { _ = self; }
    }

    inner: InnerStruct;

    proc method(self) {
        self.inner.method();
    }

    proc associated_proc() { }

    proc caller(self) {
        self.method();
        OuterStruct.associated_proc();
    }
}

test "inner type" {
    o := OuterStruct(inner: OuterStruct.InnerStruct(5));
    @assert(o.inner.abc == 5);
}
