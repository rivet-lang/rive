struct OuterStruct {
    struct InnerStruct {
        abc: int32;

        func method(self) { }
    }

    inner: InnerStruct;

    func method(self) {
        self.inner.method();
    }

    func associated_func() { }

    func caller(self) {
        self.method();
        Self::associated_func();
    }
}

test "inner type" {
    o := OuterStruct(inner: OuterStruct::InnerStruct(5));
    @assert(o.inner.abc == 5);
}
