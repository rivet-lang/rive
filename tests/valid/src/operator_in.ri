test "operator `in` with arrays" {
    @assert(true in [true, false, true]!);
    @assert(1 in [1, 2, 3]!);
    @assert("Hello" in ["Hello", "World"]!);
}

test "operator `!in` with arrays" {
    @assert(5 !in [1, 2, 3]!);
    @assert("Rivet" !in ["Hello", "World"]!);
}

test "operator `in` with vectors" {
    @assert(true in [true, false, true]);
    @assert(1 in [1, 2, 3]);
    @assert("Hello" in ["Hello", "World"]);
}

test "operator `!in` with vectors" {
    @assert(5 !in [1, 2, 3]);
    @assert("Rivet" !in ["Hello", "World"]);
}
