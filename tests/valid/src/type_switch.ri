sumtype Any {
    i32, str;
}

test "type switch" {
    let mut any = Any("string");
    switch (any) is {
        string as any_s => assert!(any_s == "string"),
        else => assert!(false)
    }

    any = Any(5);
    switch (any) is {
        i32 as any_i => assert!(any_i == 5),
        else => assert!(false)
    }
}
