test "vectors" {
    let mut vec = ["A", "B"];
    assert!(vec.len() == 2);
    vec.push("C");
    assert!(vec.len() == 3);
    let p = vec.pop();
    assert!(vec.len() == 2);
    assert!(p == "C");
}

test "vector from array" {
    let mut arr = [1, 2, 3]!;
    let mut vec = arr[..];
    vec.push(2);
    assert!(vec.len() == 4);

    let vec2 = vec.clone();
    assert!(vec2.len() == 4);
}
