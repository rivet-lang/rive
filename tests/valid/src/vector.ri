test "vector from array" {
    mut arr := [1, 2, 3]!;
    mut vec := arr[..];
    vec.push(2);
    @assert(vec.len == 4);

    vec2 := vec.clone();
    @assert(vec2.len == 4);
    @assert(vec == vec2);
}

test "`@vec` builtin function" {
    {
        mut v := @vec(int32);
        v.push(5);
        @assert(v[0] == 5);
    }
    {
        mut v := @vec(int32, 2);
        v.push(10);
        @assert(v.cap == 2);
        @assert(v[0] == 10);
    }
}
