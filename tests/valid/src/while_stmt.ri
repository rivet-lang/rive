test "`while` statement" {
    let mut x = 0;
    while x < 10 {
        x += 1;
    }
    assert!(x == 10);
}

test "`while` statement without condition (infinite loop)" {
    let mut x = 0;
    while { // infinite loop
        x += 1;
        if x == 5 {
            break;
        }
    }
    assert!(x == 5);
}
